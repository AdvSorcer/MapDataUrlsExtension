{
  "name": "MapDataUrls",
  "version": "1.0",
  "description": "為你節省每次找尋圖資的麻煩 Very Useful !",
  "permissions": ["storage","activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "120": "icon/myIcon.png"
    }
  },
  "manifest_version": 2
}
